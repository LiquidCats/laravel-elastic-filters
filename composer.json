{
    "name": "liquid-cats/laravel-elastic-filters",
    "description": "Filtration for Laravel with Elastic Search",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "I.Shabanov",
            "email": "ishabanov@liquid-cats.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0",
        "elasticsearch/elasticsearch": "^7.9"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "LiquidCats\\Filters\\": "src/LiquidCats/Filters"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\LiquidCats\\Filters\\": "tests/LiquidCats/Filters"
        }
    },
    "scripts": {
        "fix": "@php vendor/bin/php-cs-fixer fix",
        "test": "@php vendor/bin/phpunit --no-configuration tests"
    }
}
